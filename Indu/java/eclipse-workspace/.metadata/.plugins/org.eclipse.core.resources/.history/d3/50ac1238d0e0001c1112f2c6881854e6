package practice;

public class Date {
	int day;
	int month;
	int year;
	
	
	Date(int d, int m, int y){
		this.day=d;
		this.month=m;
		this.year=y;
	}
	
	void print() {
		System.out.println("[Day:"+this.day+", Month: "+this.month+", Year: "+this.year+" ]");
	}

	@Override
	public String toString() {
		return "Date [day=" + day + ", month=" + month + ", year=" + year + "]";
	}
	

	
	
}
