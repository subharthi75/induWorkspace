package practice;

import java.util.Scanner;

public class Student {

	public static void main(String[] args) {
		Scanner indu = new Scanner(System.in);
		System.out.print("Enter a number: ");
		int a = indu.nextInt();
		if(a%2 == 0) {
			System.out.println("Even");
		}else {
			System.out.println("Odd");
		}
	}

}
